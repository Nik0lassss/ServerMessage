<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nikola.chk.message_service.entity.User" table="users" catalog="messageserverdb">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="first_name" type="string">
            <column name="first_name" length="100" not-null="true" unique="true"/>
        </property>
        <property name="last_name" type="string">
            <column name="last_name" length="100" not-null="true" unique="true"/>
        </property>
        <many-to-one name="securityUser" class="com.nikola.chk.message_service.entity.SecurityUser"
                column="user_id"/>
    </class>
</hibernate-mapping>